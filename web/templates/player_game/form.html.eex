<div class="form-group">
  <select name="players" multiple id="selected_players">
    <%= for player <- @players do %>
      <option value=<%= player.id %>><%= player.name %></option>
    <% end %>
  </select>
</div>

<div class="form-group">
  <button id="create_new_game" class="btn btn-primary">Add Players</button>
</div>
